<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/spikeList">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.timetracker.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_questions"
            app:destination="@id/questions" />
    </fragment>
    <fragment
        android:id="@+id/about"
        android:name="com.example.timetracker.about"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/questions"
        android:name="com.example.timetracker.Questions"
        android:label="fragment_questions"
        tools:layout="@layout/fragment_questions" >
        <action
            android:id="@+id/action_questions_to_summary"
            app:destination="@id/summary" />
    </fragment>
    <fragment
        android:id="@+id/summary"
        android:name="com.example.timetracker.summary"
        android:label="fragment_summary"
        tools:layout="@layout/fragment_summary" />
    <fragment
        android:id="@+id/spikeList"
        android:name="com.example.timetracker.SpikeList"
        android:label="fragment_spike_list"
        tools:layout="@layout/fragment_spike_list" />
</navigation>